{
    "name": "vscode-wren",
    "displayName": "Wren",
    "description": "Syntax highlighting and snippets for the Wren programming language",
    "version": "0.1.1",
    "publisher": "nelarius",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "See the license in LICENSE.md",
    "bugs": {
        "url": "https://github.com/Nelarius/vscode-wren/issues"
    },
    "homepage": "https://github.com/Nelarius/vscode-wren/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Nelarius/vscode-wren"
    },
    "categories": [
        "Languages", "Snippets"
    ],
    "contributes": {
        "languages": [
                {
                "id": "wren",
                "aliases": ["Wren", "wren"],
                "extensions": [".wren"],
                "configuration": "./wren.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "wren",
                "scopeName": "source.wren",
                "path": "./syntaxes/wren.json"
            }
        ],
        "snippets": [
            {
                "language": "wren",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}